<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ECD7DF"
    tools:context=".BmLayout">


    <Button
        android:id="@+id/btnVideo2"
        android:layout_width="match_parent"
        android:layout_height="74dp"
        android:background="@drawable/cv3"
        android:text="VIDEO"
        app:cornerRadius="10dp"
        app:icon="@android:drawable/ic_media_play"
        app:iconPadding="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.701" />

    <Button
        android:id="@+id/btnAktiviti2"
        android:layout_width="391dp"
        android:layout_height="74dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/cv2"
        android:text="AKTIVITI"
        app:cornerRadius="10dp"
        app:icon="@android:drawable/btn_star"
        app:iconPadding="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnTajuk2" />

    <Button
        android:id="@+id/btnRangka"
        android:layout_width="391dp"
        android:layout_height="74dp"
        android:layout_marginBottom="524dp"
        android:background="@drawable/cv5"
        android:backgroundTint="@color/white"
        android:onClick="rangkakursuspdf"
        android:text="Rangka Kursus"
        app:cornerRadius="10dp"
        app:icon="@drawable/ic_baseline_filter_frames_24"
        app:iconPadding="20dp"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnSoalanExam"
        android:layout_width="391dp"
        android:layout_height="74dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/cv3"
        android:text="CONTOH SOALAN PEPERIKSAAN AKHIR"
        app:cornerRadius="10dp"
        app:icon="@android:drawable/ic_menu_save"
        app:iconPadding="20dp"
        app:layout_constraintBottom_toTopOf="@+id/btnVideo2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAktiviti2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnTajuk2"
        android:layout_width="match_parent"
        android:layout_height="74dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/cv5"
        android:text="TAJUK"
        app:cornerRadius="10dp"
        app:icon="@android:drawable/ic_menu_agenda"
        app:iconPadding="20dp"
        app:layout_constraintBottom_toTopOf="@+id/btnAktiviti2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnRangka" />

    <Button
        android:id="@+id/button2"
        android:layout_width="214dp"
        android:layout_height="42dp"
        android:layout_marginTop="51dp"
        android:backgroundTint="#D81B60"
        android:onClick="Googleform1"
        android:text="Kaji Selidik"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnVideo2"
        app:layout_constraintVertical_bias="0.611" />

</androidx.constraintlayout.widget.ConstraintLayout>